<Deployment xmlns="http://schemas.microsoft.com/client/2007/deployment" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" EntryPointAssembly="SilverLight" EntryPointType="SilverLight.App" RuntimeVersion="3.0.40624.0">
  <Deployment.OutOfBrowserSettings>
    <OutOfBrowserSettings ShortName="SilverLightTermShell Application" EnableGPUAcceleration="True" ShowInstallMenuItem="True">
      <OutOfBrowserSettings.Blurb>SilverLightTermShell Application on your desktop; at home, at work or on the go.</OutOfBrowserSettings.Blurb>
      <OutOfBrowserSettings.WindowSettings>
        <WindowSettings Title="SilverLightTermShell Application" Height="300" Width="400" />
      </OutOfBrowserSettings.WindowSettings>
      <OutOfBrowserSettings.Icons>
        <Icon Size="16,16">icon16.png</Icon>
        <Icon Size="32,32">icon32.png</Icon>
        <Icon Size="48,48">icon48.png</Icon>
        <Icon Size="128,128">icon128.png</Icon>
      </OutOfBrowserSettings.Icons>
    </OutOfBrowserSettings>
  </Deployment.OutOfBrowserSettings>
  <Deployment.Parts>
    <AssemblyPart x:Name="SilverLight" Source="SilverLight.dll" />
    <AssemblyPart x:Name="System.ComponentModel.DataAnnotations" Source="System.ComponentModel.DataAnnotations.dll" />
    <AssemblyPart x:Name="System.ComponentModel" Source="System.ComponentModel.dll" />
    <AssemblyPart x:Name="System.Runtime.Serialization" Source="System.Runtime.Serialization.dll" />
    <AssemblyPart x:Name="System.Windows.Controls.Data.DataForm" Source="System.Windows.Controls.Data.DataForm.dll" />
    <AssemblyPart x:Name="System.Windows.Controls.Data.Input" Source="System.Windows.Controls.Data.Input.dll" />
    <AssemblyPart x:Name="System.Windows.Controls" Source="System.Windows.Controls.dll" />
    <AssemblyPart x:Name="WeborbClient" Source="WeborbClient.dll" />
  </Deployment.Parts>
